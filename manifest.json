{
  "manifest_version": 3,
  "name": "testing",
  "version": "1.0",
  "description": "A testing extension!",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js", "jquery-3.7.1.min.js"]
    }
  ],
  "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
  },
  "action": {
      "default_icon": "icon16.png",
      "default_popup": "popup.html",
      "default_title": "Detect image tags"
  },
  "permissions": [],
  "background": {
    "script": ["background.js", "jquery-3.7.1.min.js"]
  }
}